    "date",
    "datetime"
  ],
  "author": "April Mintac Pineda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aprilmintacpineda/date-polyfill/issues"
  },
  "homepage": "https://github.com/aprilmintacpineda/date-polyfill#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "eslint": "^5.3.0",
    "jest": "^23.4.2",
    "prettier": "^1.14.2"
  }
