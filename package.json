{
  "name": "date-polyfill",
  "version": "1.0.9",
  "description": "Polyfill for rich date features.",
  "main": "lib/index.js",
  "files": [
    "lib/*",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "clear && npm run build && jest --coverage --env=node",
    "build": "npm run eslint && babel src --out-dir lib",
