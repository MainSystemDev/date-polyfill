    "prettier": "prettier \"src/**/*.js\" --write",
    "lint": "npm run prettier && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aprilmintacpineda/date-polyfill.git"
